{
  "AppSettings": {
    "Secret": "#{SecretKey}#",
    "AzureDevOpsToken": "#{DSMAzureDevOpsToken}#",
    "AzureDevOpsOrganizationName": "#{DSMAzureDevOpsOrganizationName}#"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DSMStorageServer": "#{DSMStorageServerKey}#",
    "DSMAuthServer": "#{DSMAuthServerKey}#"
  },
  "CacheDBSettings": {
    "DatabaseName": "#{DSMCacheDbKey}#",
    "ConnectionString": "#{DSMCacheConnectionString}#"
  },
  "FTPSettings": {
    "Host": "#{DSMFileUploadHost}#",
    "Port": "#{DSMFileUploadPort}#",
    "UserName": "#{DSMFileUploadUserName}#",
    "Password": "#{DSMFileUploadPassword}#",
    "RootPath": "#{DSMFileUploadRootPath}#",
    "UploadPath": "#{DSMFileUploadUploadPath}#"
  }
}
